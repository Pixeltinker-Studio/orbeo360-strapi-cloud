      "inversedBy": "scenes"
    },
    "map_position": {
      "type": "component",
      "component": "shared.vector2",
      "repeatable": false
    },
    "hotspots": {
      "type": "component",
      "component": "shared.hotspot",
      "repeatable": true
    }
  }
